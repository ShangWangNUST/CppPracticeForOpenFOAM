FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "/home/shuoyue/OpenFOAM/shuoyue-10/run/CppPracticeForOpenFOAM/009_Case9/testCase/-externalField";
    object      phi0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    movingWall
    {
        type            zeroGradient;
    }
    fixedWalls
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


